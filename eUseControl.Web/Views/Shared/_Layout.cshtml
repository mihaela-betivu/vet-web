<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>@ViewBag.Title - My ASP.NET Application</title>
  @Styles.Render("~/bundles/bootstrap/css")
  @Styles.Render("~/bundles/icofont/css")
  @Styles.Render("~/bundles/slick-carousel/css")
  @Styles.Render("~/bundles/assets/login")
  @Styles.Render("~/bundles/assets/css/style")

</head>
<body id="top">

<!--Header-->
@Html.Partial("_Navbar")

@RenderBody()

<!--Header-->
@Html.Partial("_Footer")

@Scripts.Render("~/bundles/jquery/js")
@Scripts.Render("~/bundles/bootstrap/js")
@Scripts.Render("~/bundles/google-map/js")
@Scripts.Render("~/bundles/slick-carousel/js")
@Scripts.Render("~/bundles/shuffle/js")
@Scripts.Render("~/bundles/assets/script/js")


@RenderSection("scripts", required: false)
</body>
</html>